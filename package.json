{
  "name": "saige-backend",
  "version": "1.0.0",
  "description": "Backend API for Saige Math SaaS",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:serverless": "tsc && cp package.json dist/",
    "start": "node dist/index.js",
    "start:serverless": "node dist/serverless.js",
    "typecheck": "tsc --noEmit",
    "create-test-users": "tsx src/scripts/create-test-user.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "stripe": "^14.17.0",
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "typescript": "^5.4.3",
    "tsx": "^4.7.2"
  }
}